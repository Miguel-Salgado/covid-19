   "source": [
    "import numpy as np\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "def reject_outliers(data, m=2):\n",
    "    return data[abs(data - np.mean(data)) < m * np.std(data)]\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[ 2  4  5  1  6  5 40] [2 4 5 1 6 5]\n"
     ]
    }
   ],
   "source": [
    "d = np.array([2,4,5,1,6,5,40])\n",
    "filtered_d = reject_outliers(d)\n",
    "print(d,filtered_d)"
   ]
  },
